#include <stdio.h>

void BubleSort(int *pa , int n){
	unsigned int i, j ;
	int temp=0;
	for(i=n-1 ; i>=0 ; i--){
		for(j=0; j<i-1 ; j++){
			if(pa[j] > pa[j+1]){
				/***swap***/
				temp=pa[j];
				pa[j]=pa[j+1];
				pa[j+1]=temp;
			}
		}
	}


}

int main(){
	int a[10]={9,2,4,6,8,7,1,2,3,58};
	BubleSort(a , 10);
	for ( int var = 0;  var <10; ++var) {
		printf("%d\n" ,a[var]);
	}


	return 0;
}

