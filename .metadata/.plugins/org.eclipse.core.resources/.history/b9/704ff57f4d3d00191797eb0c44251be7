#include <stdio.h>

void BubleSort(int *pa , int n){
	int i, j , flage=1;
	int temp=0;
	for(i=n-1 ; (i>=0 )&&flage ; i--){
		flage=0;
		for(j=i; j>0 ; j--){
			if(pa[j] < pa[j-1]){
				/***swap***/
				temp=pa[j];
				pa[j]=pa[j-1];
				pa[j-1]=temp;
				flage =1;
			}
		}
	}

}

void SelectionSort(int *pa, int n){
	 int i , j ,min ;
	int temp=0;
	for(i =0 ; i<n ; i++){
		min =i;
		for(j=i+1 ; j<n ; j++){
			if(pa[j] < pa[min]){
				min =j;
			}
		}
		/***swap***/
		temp=pa[min];
		pa[min]=pa[i];
		pa[i]=temp;

	}

}

void InselectionSort(int *pa , int n){
	int i , j , temp ;
	for(i=1 ; i<n ; i++){
		for(j=i ; j>0 ; j++){
			if(pa[j] < pa[j-1]){
				/***swap***/
				temp=pa[j-1];
				pa[j-1]=pa[j];
				pa[j]=temp;
			}
		}
	}

}


int main(){
	int a[6]={10,5,7,8,4,15};//{9,2,4,6,8,7,1,10,3,58};
	InselectionSort(a , 6);
	for ( int var = 0;  var <6; var++) {
		printf("%d\n" ,a[var]);
	}


	return 0;
}

